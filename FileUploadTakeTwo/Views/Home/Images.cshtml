@model IEnumerable<string>

<div class="row">
    <div class="col-md-6 col-md-offset-3">

        @foreach (string image in Model)
        {

            <div class="well" style="text-align: center;">
                <img src="/Uploads/@image" style="width:300px;"/>
            </div>
        }
    </div>
</div>
